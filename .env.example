# Server Configuration
PORT=7312
NODE_ENV=development
AUTO_REFRESH_SECONDS=120

# API Keys and URLs
HENRIKDEV_API_KEY=your_henrikdev_api_key_here
DISCORD_WEBHOOK_URL=
DISCORD_USER_ID_ON_ERROR=

# Player Configuration
STATS_PLAYER_NAME=YourPlayerName
STATS_PLAYER_TAG=1234
STATS_PLAYER_REGION=eu

# Security Configuration
API_SECRET_KEY=change-this-secret-key-in-production
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Performance Configuration
CACHE_TTL_SECONDS=300
API_TIMEOUT_MS=15000
MAX_LOG_ENTRIES=1000

# Logging Configuration
LOG_LEVEL=info
LOG_FILE_ENABLED=false
LOG_FILE_PATH=./logs

# Feature Flags
FEATURE_ENABLE_CACHE=true
FEATURE_ENABLE_RATE_LIMITING=true
FEATURE_ENABLE_API_DOCS=true